<div class="no-panel-padding  col-md-6 col-md-offset-7 " id="display-panel">
  <div class="page-header"><h3><%= t "workouts.display_workout.changes"%></h3></div>
  <div class="panel_body">
    <%= t "workouts.display_workout.name"%><p id="form-wo-name"></p>
    <%= t "workouts.display_workout.description"%> <p id="form-wo-desc"></p>
    <%= t "workouts.display_workout.muscle_group"%> <p id="form-muscle-group"></p>
    <h4><%= t "workouts.display_workout.exercises.title"%></h4>
    <table class="table table-striped table-bordered " id="form-exercises">
      <thead>
      <tr>
        <th><%= t "workouts.exercises.name"%></th><th><%= t "workouts.exercises.rating"%></th><th><%= t "workouts.exercises.sets"%></th><th><%= t "workouts.exercises.weight"%></th><th><%= t "workouts.exercises.reps"%></th><th><%= t "workouts.exercises.rest"%></th>
      </tr>
      </thead>
    </table>
  </div>
</div>
</div>
